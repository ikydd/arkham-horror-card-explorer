{
  "name": "blackat",
  "version": "1.0.0",
  "scripts": {
    "setup": "npm install && concurrently \"npm run install:server\" \"npm run install:client\" \"npm run install:import\"",
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "install:import": "cd import && npm install",
    "build": "npm run build:data && npm run build:client",
    "build:client": "cd client && npm run build",
    "build:data": "cd import && npm start",
    "start": "npm run server",
    "test": "concurrently \"npm run test:server\" \"npm run test:client\" \"npm run test:import\"",
    "test:client": "cd client && npm run test",
    "test:server": "cd server && npm test",
    "test:import": "cd import && npm test",
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
